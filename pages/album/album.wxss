/* pages/album/album.wxss */

.page {
  background-color: #fff;
}

.posfix {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

/* 顶部nav  */

.tapbar-top {
  width: 100%;
  height: 116rpx;
  padding: 0 40rpx;
  text-align: center;
  background-color: #fff;
  white-space: nowrap;
  /* box-shadow: 0px 2px 8px 0px rgba(0, 0, 0, 0.1); */
  box-sizing: border-box;
}

.scroll-view-tap {
  text-align: left;
}

.tapbar-top .tap-item {
  display: inline-block;
  padding-right: 60rpx;
  height: 116rpx;
  line-height: 116rpx;
  font-size: 32rpx;
  color: #999;
}

.tapbar-top .cur {
  font-weight: bold;
  color: #000;
  position: relative;
}

/* .tapbar-top .cur::after{
  content:"";
  display: block;
  width: 48rpx;
  height: 4rpx;
  background-color: #f00;
  margin-left: -24rpx;
  position: absolute;
  bottom: 4rpx;
  left: 50%;
  z-index: 9;
} */

/* 轮播  */

.switch-box {
  width: 100%;
  height: 380rpx;
  background-color: #ccc;
  overflow: hidden;
}

.album-swipe {
  width: 100%;
  height: 100%;
}

.switch-box .img-box {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.switch-box .slide-image {
  display: block;
  width: 100%;
}

/* 标题栏 */

.flow-head-box {
  width: 100%;
  position: relative;
}

.fixed-head-box {
  width: 100%;
  background-color: #fff;
  position: fixed;
  left: 0;
  top: 0 !important;
  z-index: 99;
}

.fixed-head-box .album-title-box {
  height: 136rpx;
}

.album-title-box {
  padding: 0 40rpx;
  height: 200rpx;
  background-color: #fff;
  position: relative;
}

.album-title-box .content {
  width: 100%;
  height: 100%;
  border-bottom: 1px solid #f5f5f5;
  position: relative;
}

.album-title-box .cover-content {
  height: 136rpx;
}

.album-title-box .content .head-box {
  width: 100%;
}

.album-title-box .content .title {
  width: 100%;
  font-size: 36rpx;
  line-height: 46rpx;
  color: #000;
  font-weight: 600;
  position: relative;
}

.album-title-box .content .subhead {
  width: 100%;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #333;
  position: relative;
}

.album-title-box .pos-title {
  width: calc(100% - 140rpx);
  height: 100rpx;
  padding: 30rpx;
  background-color: #fff;
  box-shadow: 0 6rpx 48rpx 0 rgba(227, 227, 227, 0.5);
  border-radius: 8rpx;
  border: 1px solid #f5f5f5;
  top: -40rpx;
  z-index: 19;
}

.pos-title .title-text {
  width: calc(100% - 40rpx);
  overflow: hidden;
}

.pos-title .title {
  width: 100%;
  color: #000;
  font-size: 36rpx;
  line-height: 46rpx;
  font-weight: 600;
}

.pos-title .subhead {
  width: 100%;
  color: #333;
  font-size: 24rpx;
  line-height: 34rpx;
}

.content .count-text {
  width: 100%;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  color: #333;
  overflow: hidden;
  position: absolute;
  bottom: 20rpx;
  left: 0;
  z-index: 2;
}

.content .fonts {
  display: block;
  float: left;
}

.content .tips {
  float: right;
  color: #999;
  overflow: hidden;
}

/* 活动精选 */

.choice-list-box {
  width: 100%;
  padding-top: 60rpx;
  background-color: #fff;
  overflow: hidden;
}

.choice-list-box .check-box {
  width: 100%;
  padding-bottom: 80rpx;
  overflow: hidden;
}

.choice-list-box .title-box {
  padding: 0 40rpx;
  height: 40rpx;
  overflow: hidden;
}

.choice-shell .name {
  width: 80%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #333;
  overflow: hidden;
}

.choice-shell .count {
  width: 20%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 20rpx;
  color: #999;
  text-align: right;
  overflow: hidden;
}

.choice-list-box .event-album-list {
  width: 100%;
  height: 280rpx;
  margin-top: 40rpx;
  white-space: nowrap;
  box-sizing: border-box;
  overflow: hidden;
}

.event-album-list .scroll-photo-list {
  width: 100%;
  height: 280rpx;
  overflow: hidden;
}

.scroll-photo-list .tap-item {
  position: relative;
  display: inline-block;
  width: 476rpx;
  height: 280rpx;
  margin-right: 20rpx;
  overflow: hidden;
}

.scroll-photo-list .tap-item:first-child {
  margin-left: 40rpx;
}

/* 相册照片列表  */

.photo-list {
  width: 100%;
  padding: 0 40rpx;
  padding-bottom: 10rpx;
  overflow: hidden;
  box-sizing: border-box;
}

.item-two {
  width: 50%;
  height: 206rpx;
  box-sizing: border-box;
  padding: 3rpx;
  overflow: hidden;
  float: left;
  position: relative;
}

.item-three {
  width: 33.33%;
  height: 206rpx;
  box-sizing: border-box;
  padding: 6rpx;
  overflow: hidden;
  float: left;
  position: relative;
}

.list-image {
  width: 100%;
  height: 100%;
}

.icon-select {
  width: 100%;
  height: 100%;
  /* background: rgba(255, 255, 255, 0.4); */
  position: absolute;
  left: 0;
  top: 0;
  z-index: 2;
}

.icon-select .icon {
  position: absolute;
  right: 14rpx;
  top: 14rpx;
  z-index: 3;
  font-size: 28rpx;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}

.icon-select .text-icon {
  border-radius: 50rpx;
  background-color: #fff;
  color: #ea111c;
}

.topTen-item {
  width: 100%;
  height: 500rpx;
  margin-top: 40rpx;
  position: relative;
}

.topTen-item:first-child {
  margin-top: 0;
}

.top-text {
  display: block;
  width: 80rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  position: absolute;
  top: 20rpx;
  left: -20rpx;
  z-index: 9;
}

.top-like {
  min-width: 44rpx;
  height: 36rpx;
  line-height: 36rpx;
  padding: 0 20rpx;
  font-size: 26rpx;
  color: #fff;
  text-align: center;
  background-color: rgba(34, 34, 34, 0.6);
  border-radius: 18rpx;
  position: absolute;
  bottom: 20rpx;
  left: 20rpx;
  z-index: 9;
  vertical-align: middle;
}

.top-like .img {
  display: block;
  width: 24rpx;
  height: 24rpx;
  padding-right: 6rpx;
  padding-top: 6rpx;
  float: left;
}

.top-like .text {
  display: block;
  float: left;
  line-height: 36rpx;
}

.one {
  background-color: #faca2b;
}

.two {
  background-color: #a6b8c2;
}

.three {
  background-color: #f3903c;
}

/* layou button  */

.layou-box {
  width: 78rpx;
  padding-bottom: 98rpx;
  position: fixed;
  right: 20rpx;
  bottom: 60rpx;
  z-index: 99;
}

.layou-one {
  padding-bottom: 0;
}

.layou-box .bth {
  width: 76rpx;
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
  color: #333;
  font-size: 20rpx;
  font-weight: 700;
  background-color: #fff;
  border-radius: 50%;
  /* box-shadow: 0px 2px 4px 0px rgba(0,0,0,0.2);  */
  box-shadow: 0px 2px 15px 0px rgba(227, 227, 227, 0.5);
  border: 1rpx solid #f3f3f3;
}

.layou-box .img {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  overflow: hidden;
}

.layou-box .images {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  margin-top: 20rpx;
  overflow: hidden;
}

.layou-box .position {
  width: 226rpx;
  height: 76rpx;
  background-color: #fff;
  /* box-shadow: 0px 2px 4px 0px rgba(0,0,0,0.2);  */
  box-shadow: 0px 2px 15px 0px rgba(227, 227, 227, 0.5);
  border-radius: 38px;
  position: absolute;
  right: 0;
  bottom: 0;
}

.layou-box .close {
  width: 76rpx;
}

.layou-box .position .text {
  display: block;
  width: 76rpx;
  line-height: 76rpx;
  font-size: 20rpx;
  color: #555;
  text-align: center;
  float: left;
  position: relative;
}

.layou-box .position .gray {
  color: #ccc;
}

.layou-box .position .text::after {
  content: "|";
  color: #e3e3e3;
  position: absolute;
  right: 0;
}

.layou-box .position .setting {
  position: absolute;
  right: 0;
  top: 0;
  z-index: 9;
}

.layou-box .position .setting::after {
  color: transparent;
}

/* timeline  */

.timeline-list {
  width: 100%;
  padding: 0 58rpx;
  box-sizing: border-box;
  overflow: hidden;
}

.timeline-box {
  padding-top: 30rpx;
}

.timeline-title {
  height: 48rpx;
  line-height: 48rpx;
  color: #000;
  font-size: 32rpx;
  font-weight: bold;
}

.time-box {
  overflow: hidden;
}

.time-title {
  height: 40rpx;
  line-height: 40rpx;
  color: #4d4d4d;
  font-size: 28rpx;
  margin-bottom: 30rpx;
}

.time-item {
  width: 25%;
  height: 152rpx;
  box-sizing: border-box;
  padding: 3rpx;
  overflow: hidden;
  float: left;
  position: relative;
}

/* 拼图底部弹层  */

.layou-puzzle {
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  font-size: 36rpx;
  background-color: rgba(255, 255, 255, 1);
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 19;
}

.layou-puzzle .left-bth {
  padding: 0 60rpx;
  height: 98rpx;
  float: left;
}

.layou-puzzle .right-bth {
  right: 20rpx;
  padding: 0 40rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  border-radius: 50rpx;
  background-color: #ea111c;
  color: #fff;
  float: right;
}

.layou-puzzle .opacity {
  opacity: 0.5;
}

.layou-puzzle .red-bg {
  display: inline-block;
  width: 44rpx;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  border-radius: 50%;
  margin: 27rpx 8rpx;
  background-color: rgba(234, 17, 28, 1);
}

.merge-loading {
  width: 100%;
  height: 100%;
  padding-top: 40%;
  font-size: 24rpx;
  line-height: 66rpx;
  color: #999;
  text-align: center;
  background-color: #fff;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 199;
}

.merge-loading .img {
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto;
}

.index-tips-box {
  width: 100%;
  padding-top: 40rpx;
  text-align: center;
  color: #999;
  font-size: 28rpx;
}

.index-tips-box .bg-img {
  width: 372rpx;
  height: 372rpx;
  margin: 0 auto;
  overflow: hidden;
}

.index-tips-box .tips-text {
  display: block;
  width: 100%;
  height: 116rpx;
  line-height: 116rpx;
  overflow: hidden;
}

/* 大图弹层  */

.popup-box {
  width: 100%;
  height: 100%;
  background-color: #000;
  overflow: hidden;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99;
}

.popup-name {
  width: 100%;
  padding: 40rpx;
  overflow: hidden;
  box-sizing: border-box;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99;
  background: linear-gradient(180deg, rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0));
}

.visitors-number {
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #ccc;
  margin-top: 20rpx;
  overflow: hidden;
}

.visitors-number .count {
  color: #fff;
  font-weight: 600;
}

.popup-name .left_b {
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #ccc;
}

.popup-name .left_b .auth {
  width: 30rpx;
  height: 30rpx;
  padding-right: 10rpx;
  overflow: hidden;
}

.popup-name .right_b {
  width: 140rpx;
  height: 58rpx;
  overflow: hidden;
  position: absolute;
  right: 40rpx;
  top: 30rpx;
  z-index: 9;
}

.right_b image {
  padding: 10rpx;
  display: inline-block;
  width: 38rpx;
  height: 38rpx;
}

.popup-swipe {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.big-switch-item {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.popup-swipe .img-box {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  /* padding:200rpx 0; */
  overflow: scroll;
  display: flex;
  align-items: center;
  justify-content: center;
}

.popup-swipe .slide-image {
  width: 100%;
  height: 100%;
}

.animate-show {
  animation: 1.5s opacity1 0s;
}

.animate-hide {
  animation: 1.5s opacity0 0s;
}

@keyframes opacity1 {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

@keyframes opacity0 {
  0% {
    opacity: 1;
  }

  100% {
    opacity: 0;
  }
}

/* 小轮播  */

.popup-swipe-small {
  width: 100%;
  height: 416rpx;
  background: linear-gradient(180deg, rgba(0, 0, 0, 0), rgba(0, 0, 0, 1));
  opacity: 0.9;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 98;
}

.popup-swipe-small .title-box {
  padding: 30rpx 40rpx;
  height: 134rpx;
  color: #fff;
  overflow: hidden;
}

.popup-swipe-small .title-box .title {
  display: block;
  height: 68rpx;
  line-height: 68rpx;
  font-size: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.popup-swipe-small .title-box .content {
  height: 66rpx;
  line-height: 32rpx;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.scl-box {
  width: 100%;
}

.popup-scroll-box {
  width: 100%;
  height: 94rpx;
  text-align: center;
  white-space: nowrap;
}

.popup-scroll-box .item-box {
  display: inline-block;
  width: 94rpx;
  height: 94rpx;
  padding: 0 2rpx;
  box-sizing: border-box;
}

.popup-scroll-box .scroll-image {
  width: 100%;
  height: 100%;
}

.popup-scroll-box .cur .scroll-image {
  border: 1px solid #fff;
  box-sizing: border-box;
}

.popup-tool {
  /* display: flex; */
  width: 100%;
  padding-left: 24rpx;
  padding-bottom: 30rpx;
  padding-right: 24rpx;
  height: 90rpx;
  line-height: 70rpx;
  color: #fff;
  font-size: 28rpx;
  box-sizing: border-box;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 99;
  overflow: hidden;
}

.popup-tool .flex {
  display: block;
  /* flex: 1;  */
  min-width: 100rpx;
  height: 30rpx;
  line-height: 30rpx;
  padding: 20rpx;
  text-align: center;
  position: relative;
  float: left;
}

.popup-tool .like {
  float: right;
}

.popup-tool .img {
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  margin-top: 1rpx;
  padding-right: 10rpx;
  overflow: hidden;
  vertical-align: middle;
}

.popup-tool .like .img {
  height: 28rpx;
  margin-top: 1rpx;
}

.popup-tool .text {
  display: inline-block;
  vertical-align: middle;
}

.share-bth {
  width: 100%;
  height: 100%;
  opacity: 0;
  padding: 0;
  margin: 0;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 5;
}

/* 照片信息  */

.pic-info {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.9);
  overflow: hidden;
  box-sizing: border-box;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 199;
}

.pic-info .info-bth {
  width: 100%;
  height: 68rpx;
  padding: 40rpx 0;
  overflow: hidden;
}

.info-bth image {
  padding: 10rpx;
  display: block;
  width: 38rpx;
  height: 38rpx;
  margin-right: 122rpx;
  float: right;
}

.pic-list .item {
  width: 100%;
  height: 68rpx;
  line-height: 68rpx;
  font-size: 24rpx;
  color: #fff;
  overflow: hidden;
}

.pic-list text {
  display: block;
  float: left;
}

.pic-list .left {
  width: 150rpx;
  padding-right: 40rpx;
  color: #ccc;
  text-align: right;
}

.gallery-view-panel {
  flex: 1;
  width: 100%;
  height: calc(100% - 100rpx);
  position: relative;
}

.gallery-view-panel .title-box {
  position: absolute;
  display: inline-block;
  width: 100%;
  height: 134rpx;
  padding: 0 0 0 40rpx;
  color: #fff;
  overflow: hidden;
  left: 0rpx;
  bottom: 85rpx;
}

.gallery-view-panel .title-box .title {
  display: block;
  height: 68rpx;
  line-height: 68rpx;
  font-size: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.gallery-view-panel .title-box .content {
  height: 66rpx;
  line-height: 32rpx;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

/* 视频列表 */

.video-list {
  padding: 0 54rpx;
}

.video-item {
  margin-bottom: 80rpx;
}

.video-item .video-box {
  position: relative;
  width: 100%;
  height: 360rpx;
}

.video-item .video {
  width: 100%;
  height: 100%;
}

.video-item .video-time {
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  z-index: 2;
  color: #fff;
  background-color: #000;
  width: 106rpx;
  height: 36rpx;
  font-size: 24rpx;
  line-height: 36rpx;
  text-align: center;
}

.video-item .video-modal {
  position: absolute;
  top: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  line-height: 360rpx;
  color: #fff;
  text-align: center;
  font-size: 40rpx;
  background-color: rgba(0, 0, 0, 0.5);
}

.video-modal.hide-modal {
  z-index: 11;
}

.video-modal .v-time {
  top: 200rpx;
  z-index: 5;
  width: 100rpx;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 24rpx;
}

.video-modal .video-play-icon {
  z-index: 5;
  width: 32rpx;
  height: 32rpx;
}

.video-modal .video-bg-image {
  z-index: 4;
  width: 100%;
  height: 100%;
}

.video-item .v-title {
  margin-bottom: 10rpx;
  margin-top: 20rpx;
  font-size: 32rpx;
  font-weight: 700;
}

.video-item .video-detail {
  margin-bottom: 20rpx;
  font-size: 24rpx;
  color: #999;
}

.video-detail .v-time {
  margin-left: 26rpx;
}

.video-item .v-desc {
  font-size: 28rpx;
}

.video-item .v-visit {
  font-size: 24rpx;
  color: #999;
}

.video-item .v-visit .fr {
  margin-left: 26rpx;
}

.video-item .btm-contain {
  height: 80rpx;
  line-height: 80rpx;
}

.video-item .v-details {
  padding: 0 20rpx;
  font-size: 28rpx;
}
@import '../../components/galleryview/galleryview.wxss';
